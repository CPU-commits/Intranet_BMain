    "nats": "^2.13.1",
    "nest-winston": "^1.9.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.0",
    "rxjs": "^7.8.1",
    "uuid": "^9.0.0",
    "winston": "^3.8.2",
    "xlsx": "^0.18.5"
